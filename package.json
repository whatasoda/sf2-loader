  "repository": {
    "type": "git",
    "url": "git+https://github.com/whatasoda/sf2-loader.git"
  },
  "keywords": [
    "sf2",
    "soundfont",
    "webpack",
    "loader"
  ],
  "author": "whatasoda <git@whatasoda.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whatasoda/sf2-loader/issues"
  },
  "homepage": "https://github.com/whatasoda/sf2-loader#readme",
  "dependencies": {
    "loader-utils": "^3.2.1",
    "node-lame": "^1.2.0",
